func main() {
    let number = 10;
    let result = fibonacci(number);
    println(result);
}

func fibonacci(n: int) -> int {
    if n < 0 {
        return -1;
    } else if n == 0 or n == 1 {
        return 1;
    } else {
        let first = 1;
        let second = 1;
        for let i = 2; i <= n; i += 1 {
            let temp = first;
            first = second;
            second += temp;
        }
        return first;
    }
}

